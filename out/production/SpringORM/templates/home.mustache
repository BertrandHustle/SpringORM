<html>
<body>

<table border = "1">
    <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Category</th>
        <th>Credit Card#</th>
        <th>CVV</th>
        <th>Date</th>
    </tr>
    {{#customers}}
    <tr>
        <td>
            {{name}}
        </td>
        <td>
            {{email}}
        </td>
    </tr>
    {{/customers}}

    {{#purchases}}
    <tr>
        <td>
            {{category}}
        </td>
        <td>
            {{creditCard}}
        </td>
        <td>
            {{cvv}}
        </td>
        <td>
            {{date}}
        </td>
    </tr>
    {{/purchases}}

</table>
</body>
</html>

